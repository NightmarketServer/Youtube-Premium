[Rule]
AND,((DOMAIN-SUFFIX,googlevideo.com), (PROTOCOL,UDP)),REJECT
AND,((DOMAIN,youtubei.googleapis.com), (PROTOCOL,UDP)),REJECT

[Url Rewrite]
# /Modules/YT-Adblocker.module
# ^https?:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad - reject-img
^https?:\/\/[\w-]+\.googlevideo\.com\/(?!(dclk_video_ads|videoplayback\?)).+&oad _ 302
# ======= Universal Ad Request ======= #
^https?:\/\/(www|s)\.youtube\.com\/api\/stats\/ads _ 302
^https?:\/\/(www|s)\.youtube\.com\/(pagead|ptracking) _ 302
^https?:\/\/s\.youtube\.com\/api\/stats\/qoe\?adcontext _ 302
# ======= General Video Ad request ======= #
(^https?:\/\/[\w-]+\.googlevideo\.com\/(?!dclk_video_ads).+?)&ctier=L(&.+?),ctier,(.+) $1$2$3 302
^https:\/\/youtubei\.googleapis\.com\/(youtubei\/v1\/(browse|next|player|search|reel\/reel_watch_sequence|guide|account\/get_setting|get_watch))(\?(.*))?$ {{{Y√™u C·∫ßu D·ªãch V·ª• X·ª≠ L√Ω}}}/$1?blockUpload={{{·∫®n N√∫t T·∫£i L√™n}}}&blockImmersive={{{N√∫t Ch·ªçn Kh·ªëi}}}&blockShorts={{{·∫®n N√∫t Shorts}}}&captionLang={{{C√†i Ph·ª• ƒê·ªÅ}}}&lyricLang={{{Ê≠åËØçÁøªËØëËØ≠Ë®Ä}}}&$4 307

[Map Local]
^https?:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad data-type=text data="" status-code=200

